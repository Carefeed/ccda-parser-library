{
  "name": "uhin/ccda-parser-library",
  "description": "A library providing data objects to interact with Consolidated Clinical Document Architecture (C-CDA) data.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Mark Chavez",
      "email": "mchavez@uhin.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "ext-simplexml": "*",
    "php": "^7.4|^8.0|^8.1|^8.2|^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "fakerphp/faker": "^1.9",
    "php-mock/php-mock-phpunit": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Uhin\\Ccda\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uhin\\Ccda\\Tests\\": "tests/"
    }
  }
}
